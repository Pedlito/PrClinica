@model IEnumerable<BD_PR_01_Clinicas.Models.tbEntrada>

@{
    ViewBag.Title = "Index";
}

<h2>Entradas</h2>

<p>
    @Html.ActionLink("Nueva entrada", "Crear")
</p>
<table class="table table-striped table-hover table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaEntrada)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaEntrada)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.codEntrada }) |
            @Html.ActionLink("Detailles", "Details", new { id=item.codEntrada }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.codEntrada })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Regresar", "Administracion", "Home", null, htmlAttributes: new { @class = "btn btn-primary" })
</div>