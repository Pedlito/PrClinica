@model IEnumerable<BD_PR_01_Clinicas.Models.tbEntrada>

@{
    ViewBag.Title = "Index";
}

<h2>Entradas</h2>

<p>
    @Html.ActionLink("Nueva Entrada", "Crear")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Descripción")
        </th>
        <th>
            @Html.DisplayName("Fecha de Entrada")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td class="col-md-5">
            @Html.DisplayFor(modelItem => item.descripcion)
        </td>
        <td>
            @item.fechaEntrada.Value.ToLongDateString()
        </td>
        <td>
            @Html.ActionLink("Detalles", "Detalles", new { codEntrada = item.codEntrada })
        </td>
    </tr>
}

</table>
