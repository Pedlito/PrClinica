@model IEnumerable<BD_PR_01_Clinicas.Models.tbRotacion>

@{
    ViewBag.Title = "Index";
}

<h2>Rotaciones</h2>

<p>
    @Html.ActionLink("Nueva", "Crear")
</p>
<table class="table table-striped table-hover">
    <thead>
        <tr class="bg-info">
            <th>
                @Html.DisplayName("Fecha de Inicio")
            </th>
            <th>
                @Html.DisplayName("Fecha de Finalización")
            </th>
            <th></th>
        </tr>
    </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.fechaInicio.Value.ToLongDateString()
                </td>
                <td>
                    @item.fechaFinal.Value.ToLongDateString()
                </td>
                <td>
                    @Html.ActionLink("Integrantes", "Integrantes", new { id = item.codRotacion }) 
                    @*|
                    @Html.ActionLink("Archivar", "Delete", new { /* id=item.codRotacion*/ })*@
                </td>
            </tr>
        }

</table>
<div>
    @Html.ActionLink("Regresar", "Administracion", "Home", null, htmlAttributes: new { @class = "btn btn-primary" })
</div>